{
	"compilerOptions": {
	  "target": "es5",
	  "module": "commonjs",
	  "strict": true,
	  "noImplicitAny": true,
	  "esModuleInterop": true, 
	  "strictNullChecks": false,
	  "allowUnreachableCode": true,
	  "sourceMap": true,
	  "strictPropertyInitialization": false,
	  "typeRoots": ["node_modules/@types", "typings"],
	  "lib": ["dom", "es2015.promise", "es5"],
	}
  }
  